{
  "custom": {
    "UserName": "Yogini"
  },
  "params": {
    "backID": "Congregations",
    "locationId": -1,
    "tab_id": 1
  },
  "propConfig": {
    "custom.UserName": {
      "binding": {
        "config": {
          "path": "session.props.auth.user.firstName"
        },
        "type": "property"
      },
      "persistent": true
    },
    "params.backID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.locationId": {
      "binding": {
        "config": {
          "parameters": {
            "providerName": "{view.custom.UserName}"
          },
          "queryPath": "WinterShelter/Get_LocationId_ByUserName"
        },
        "transforms": [
          {
            "code": "\tif value.getRowCount() \u003e 0:\n\t\tlocationId \u003d value.getValueAt(0,0)\n\telse:\n\t\tlocationId \u003d -1\n\treturn locationId",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "paramDirection": "inout",
      "persistent": true
    },
    "params.tab_id": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 900,
      "width": 1273
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Header"
        },
        "position": {
          "basis": "90px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.backID": {
            "binding": {
              "config": {
                "path": "view.params.backID"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "tabSelected": null
          },
          "path": "WinterShelter/Detail/Cards/CongregationDetail_Header"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Tabs"
        },
        "position": {
          "basis": "810px",
          "grow": 1
        },
        "propConfig": {
          "props.params.data": {
            "binding": {
              "config": {
                "parameters": {
                  "ID": "{view.params.CongregationID}"
                },
                "queryPath": "WinterShelter/Details/Congregation_ID"
              },
              "type": "query"
            }
          },
          "props.params.locationID": {
            "binding": {
              "config": {
                "path": "view.params.locationId"
              },
              "type": "property"
            }
          },
          "props.params.tab_id": {
            "binding": {
              "config": {
                "path": "view.params.tab_id"
              },
              "transforms": [
                {
                  "code": "\treturn int(value)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "path": "WinterShelter/Detail/Cards/CongregationDetail_Tabs"
        },
        "type": "ia.display.view"
      }
    ],
    "custom": {
      "key": "value"
    },
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}