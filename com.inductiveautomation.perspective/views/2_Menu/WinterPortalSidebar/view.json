{
  "custom": {
    "key": "0"
  },
  "params": {},
  "propConfig": {
    "custom.key": {
      "binding": {
        "config": {
          "path": "session.custom.congregationID"
        },
        "transforms": [
          {
            "code": "\tif value is not None:\n\t\turl \u003d \"/WinterShelter/Congregations/Detail/\" + str(value)\n\t\tsystem.perspective.navigate(page \u003d url)\n\treturn value",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 900,
      "width": 251
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Image"
            },
            "position": {
              "basis": "100px"
            },
            "props": {
              "fit": {
                "mode": "contain"
              },
              "source": "/system/images/yourLogo.png"
            },
            "type": "ia.display.image"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "Spacer"
                },
                "position": {
                  "basis": "20px"
                },
                "type": "ia.display.label"
              }
            ],
            "meta": {
              "name": "FlexContainer"
            },
            "position": {
              "basis": "60px",
              "grow": 1,
              "shrink": 0
            },
            "props": {
              "style": {
                "backgroundColor": "#115999"
              }
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "Titles"
        },
        "position": {
          "basis": "185px",
          "grow": 1,
          "shrink": 0
        },
        "props": {
          "direction": "column",
          "justify": "space-between",
          "style": {
            "overflow": "visible"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {},
                  "scope": "C",
                  "type": "logout"
                }
              }
            },
            "meta": {
              "name": "Image",
              "tooltip": {
                "enabled": true,
                "text": "Logout"
              }
            },
            "position": {
              "basis": "25px"
            },
            "props": {
              "fit": {
                "mode": "fill"
              },
              "source": "/system/images/SVGs/No Picture.svg",
              "style": {
                "cursor": "pointer"
              }
            },
            "type": "ia.display.image"
          },
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "149px"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "if({session.props.device.type} \u003d \u0027designer\u0027,\u0027John Gerbac\u0027,\r\n{session.props.auth.user.firstName} + \u0027 \u0027 + {session.props.auth.user.lastName})"
                  },
                  "type": "expr"
                },
                "onChange": {
                  "enabled": null,
                  "script": "\tlogger \u003d system.util.getLogger(\"myLogger\")\n\tlogString \u003d \u0027User \u0027 + str(self.session.props.auth.user.userName) + \u0027 has logged in to a Perspective session.\u0027\n\tlogger.info(logString)"
                }
              }
            },
            "props": {
              "style": {
                "color": "#c5d4e3",
                "fontFamily": "Proxima Nova ExCn",
                "fontSize": "20px",
                "fontStyle": "italic",
                "paddingLeft": "5px",
                "textAlign": "Left"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Image_1"
            },
            "position": {
              "basis": "8px"
            },
            "type": "ia.display.image"
          }
        ],
        "meta": {
          "name": "Administration"
        },
        "position": {
          "basis": "80px"
        },
        "props": {
          "style": {
            "overflow": "visible",
            "paddingBottom": "20px",
            "paddingLeft": "10px",
            "paddingRight": "10px",
            "paddingTop": "5px"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "backgroundColor": "#0c4e8b"
      }
    },
    "type": "ia.container.flex"
  }
}