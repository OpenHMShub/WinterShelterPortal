          "props.params.selectedTab": {
            "binding": {
              "config": {
                "path": "view.params.tab_id"
              },
              "transforms": [
                {
                  "fallback": "Activity",
                  "inputType": "expression",
                  "mappings": [
                    {
                      "input": "\"1\"",
                      "output": "Activity"
                    },
                    {
                      "input": "\"10\"",
                      "output": "Registration"
                    },
                    {
                      "input": "\"20\"",
                      "output": "Intake"
                    },
                    {
                      "input": "\"30\"",
                      "output": "Referrals"
                    },
                    {
                      "input": "\"40\"",
                      "output": "Case Notes"
                    },
                    {
                      "input": "\"50\"",
                      "output": "Programs"
                    },
                    {
                      "input": "\"60\"",
                      "output": "Services"
                    },
                    {
                      "input": "\"70\"",
                      "output": "Drug Screen"
                    },
                    {
                      "input": "\"80\"",
                      "output": "Incidents"
                    },
                    {
                      "input": "\"90\"",
                      "output": "Reinstatement"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "path": "Participants/Detail/Components/TabStrip/TabStrip",
