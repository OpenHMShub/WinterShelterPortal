{
  "custom": {},
  "params": {
    "partial": false,
    "participantID": 2,
    "suspended": false,
    "tab_id": 0
  },
  "propConfig": {
    "params.partial": {
      "binding": {
        "config": {
          "path": "/root/Content/PersonalInformation.props.params.partial"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.participantID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.suspended": {
      "binding": {
        "config": {
          "path": "/root/Content/PersonalInformation.props.params.suspended"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.tab_id": {
      "paramDirection": "input",
      "persistent": true
    }
  },
