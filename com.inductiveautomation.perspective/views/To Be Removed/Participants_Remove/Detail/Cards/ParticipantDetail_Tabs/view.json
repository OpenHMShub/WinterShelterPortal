        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "4px",
          "shrink": 0
        },
        "props": {
          "style": {
            "classes": "Background/Alternative"
          }
        },
        "type": "ia.display.label"
      },
      {
        "children": [
          {
            "meta": {
              "name": "PersonalInformation"
            },
            "position": {
              "basis": "250px",
              "shrink": 0
            },
            "propConfig": {
              "props.params.participantID": {
                "binding": {
                  "config": {
                    "path": "view.params.participantID"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "params": {
                "partial": false,
                "suspended": false
              },
              "path": "Participants/Detail/Cards/PersonalInformation",
              "style": {
                "overflow": "auto"
              }
            },
            "type": "ia.display.view"
          },
          {
            "meta": {
              "name": "Content"
            },
            "position": {
              "basis": "300px",
              "grow": 1
            },
            "propConfig": {
              "props.params.participantID": {
                "binding": {
                  "config": {
                    "path": "view.params.participantID"
                  },
                  "type": "property"
                }
              },
              "props.path": {
                "binding": {
                  "config": {
                    "path": ".../Tabstrip.props.params.selectedTab"
                  },
                  "transforms": [
                    {
                      "fallback": null,
                      "inputType": "scalar",
                      "mappings": [
                        {
