{
  "custom": {},
  "params": {
    "Text": "Shelter"
  },
  "propConfig": {
    "params.Text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 35,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "20px"
        },
        "props": {
          "color": "#0C4E8B",
          "path": "material/keyboard_arrow_down",
          "style": {
            "marginLeft": "15px",
            "marginTop": "2px"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "50%",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Summary_Title"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
}