{
  "custom": {},
  "params": {
    "func_select_participant": false
  },
  "propConfig": {
    "params.func_select_participant": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 47,
      "width": 1100
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\t#Trigger the add participant script\n\t#system.perspective.print(self.view.params.func_add_participant)\n\tself.view.params.func_select_participant \u003d True"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "button_select"
        },
        "position": {
          "basis": "140px"
        },
        "props": {
          "style": {
            "classes": "Buttons/Secondary",
            "marginBottom": "5px",
            "marginTop": "5px"
          },
          "text": "select participant"
        },
        "type": "ia.input.button"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\tmessageType \u003d \u0027ResetParticipantSearch\u0027\n\tsystem.perspective.sendMessage(messageType)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "button_new"
        },
        "position": {
          "basis": "140px"
        },
        "props": {
          "style": {
            "classes": "Buttons/Secondary",
            "marginBottom": "5px",
            "marginLeft": "10px",
            "marginTop": "5px"
          },
          "text": "clear search"
        },
        "type": "ia.input.button"
      },
      {
        "meta": {
          "name": "Embed_Placeholder_2"
        },
        "position": {
          "basis": "1px",
          "grow": 1
        },
        "props": {
          "path": "Participants/Templates/Placeholder"
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
}