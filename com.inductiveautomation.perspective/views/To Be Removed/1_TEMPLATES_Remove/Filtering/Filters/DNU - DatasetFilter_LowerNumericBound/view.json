{
  "custom": {},
  "params": {
    "clear": false,
    "column_name": "Gender",
    "options": [],
    "placeholder": "Gender"
  },
  "propConfig": {
    "params.clear": {
      "onChange": {
        "enabled": null,
        "script": "\tif self.params.clear:\n\t\tself.getChild(\"Filter\").getChild(\"filter\").props.value \u003d None\n\tself.params.clear \u003d False"
      },
      "paramDirection": "inout",
      "persistent": true
    },
    "params.column_name": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.options": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.placeholder": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 309
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "filter"
        },
        "position": {
          "basis": "220px",
          "grow": 1
        },
        "propConfig": {
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.options"
              },
              "type": "property"
            }
          },
          "props.placeholder.text": {
            "binding": {
              "config": {
                "path": "view.params.placeholder"
              },
              "type": "property"
            }
          },
          "props.style.classes": {
            "binding": {
              "config": {
                "path": "view.params.selection"
              },
              "transforms": [
                {
                  "code": "\ttext \u003d \"Filter/Filter_Text_Default\"\n\tif value !\u003d None:\n\t\tif value !\u003d \"\" and value \u003e -1:\n\t\t\ttext \u003d \"Filter/Filter_Text_TeelBlue\"\n\treturn text",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.value": {
            "onChange": {
              "enabled": null,
              "script": "\tpageId \u003d filters.sanitizePageID(self.page.props.pageId)\n\t#system.perspective.print(\u0027Search: %s --\u003e %s\u0027 % (self.page.props.pageId, pageId))\n\t# Update the session\u0027s filter listing for this page\n\tkey \u003d self.view.params.column_name\n\tselection \u003d currentValue.value\n\tif selection !\u003d None:\n\t\tselection \u003d self.props.value\n\n\tself.session.custom.filters[pageId] \u003d filters.manageFilterList(\n\t\tself.session.custom.filters.get(pageId, []),\n\t\t\u0027minBound\u0027,\n\t\tkey,\n\t\tselection or None,\n\t\t\u0027auto\u0027\n\t\t)\n"
            }
          }
        },
        "props": {
          "dropdownOptionStyle": {
            "classes": "Filter/Filter_Text_Default"
          },
          "placeholder": {
            "color": "#AAAAAA"
          },
          "showClearIcon": true,
          "style": {},
          "value": null,
          "wrapMultiSelectValues": false
        },
        "type": "ia.input.dropdown"
      }
    ],
    "meta": {
      "name": "Filter"
    },
    "propConfig": {
      "props.style.classes": {
        "binding": {
          "config": {
            "path": "view.params.selection"
          },
          "transforms": [
            {
              "code": "\tborder \u003d \"Filter/Filter_Border_Default\"\n\tif value !\u003d None:\n\t\tborder \u003d \"Filter/Filter_Border_TeelBlue\"\n\treturn border",
              "type": "script"
            }
          ],
          "type": "property"
        }
      }
    },
    "props": {
      "alignItems": "center",
      "style": {}
    },
    "type": "ia.container.flex"
  }
}